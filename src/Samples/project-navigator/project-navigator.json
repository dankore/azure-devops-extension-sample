{
    "contributions": [
        {
            "id": "project-navigator-widget",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog"
            ],
            "properties": {
                "name": "Project Navigator",
                "description": "Quickly navigate to any of your Azure DevOps projects from your dashboard.",
                "catalogIconUrl": "static/project-navigator-logo.png",
                "uri": "dist/project-navigator/project-navigator.html",
                "isNameConfigurable": true,
                "supportedSizes": [
                    { "rowSpan": 2, "columnSpan": 2 },
                    { "rowSpan": 2, "columnSpan": 3 },
                    { "rowSpan": 3, "columnSpan": 2 },
                    { "rowSpan": 3, "columnSpan": 3 }
                ],
                "supportedScopes": [ "project_team" ]
            }
        }
    ],
    "scopes": [
        "vso.project"
    ]
}
